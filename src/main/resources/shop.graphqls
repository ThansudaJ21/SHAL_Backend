    shopId:Int
}
type QueryFailureReasonDto {
    id:Int
    text: String
    failureReasons: FailureListDto
    shopId:Int
}
type FailureReasonDto {
    id: Int
    reason: String
}
type FailureListDto {
    reason: String
}

input InputFailureReason {
    id: Int
    text: String
